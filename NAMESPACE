## Import functions
importFrom("stats", "na.omit")
importFrom("utils", "object.size")
importFrom("vegdata", "tv.home")

## Import of packages
import(
	foreign,
	methods
)

## Export of classes
exportClasses(
	taxlist
)

## Export methods
exportMethods(
	"[", "[<-", "$", "$<-",
	accepted_name,"accepted_name<-",
	add_concept,
	add_level,"add_level<-",
	add_parent,"add_parent<-",
	add_synonym,
	add_view,
	"change_concept<-",
	clean,
	df2taxlist,
	get_children,
	get_parents,
	levels,"levels<-",
	taxon_names,"taxon_names<-",
	taxon_relations,"taxon_relations<-",
	taxon_traits,"taxon_traits<-",
	taxon_views,"taxon_views<-",
	subset,
	summary
)

# Export of regular function names
export(
	backup_object,
	tv2taxlist
)
